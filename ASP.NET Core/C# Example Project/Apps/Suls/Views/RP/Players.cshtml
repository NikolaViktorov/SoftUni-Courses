<div class="row">
    <div class="jumbotron col-md-12">
        <div class="heading-container bottom-buffer-md">
            <h1>Online player names: @Model.PlayersCount</h1>
        </div>

        @foreach (var name in Model.PlayerNames)
        {
            <p class="">@name</p>
        }

        <div class="button-holder">
            <a href="/RP/Home" class="btn btn-info text-uppercase">Go back</a>
        </div>
    </div>
</div>
